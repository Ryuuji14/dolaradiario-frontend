{
  "name": "dolaradiario",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "watch:tailwind": "postcss src/tailwind.css -o public/final.css -w",
    "build:tailwind": "cross-env NODE_ENV=production postcss src/tailwind.css -o final/index.css",
    "build": "npm run build:tailwind && rollup -c",
    "start": "sirv public --host 0.0.0.0",
    "serve": "serve public -p 80",
    "dev": "concurrently \"rollup -c -w\" \"npm run watch:tailwind\"",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@beyonk/svelte-carousel": "^2.8.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "postcss": "^8.2.13",
    "postcss-cli": "^8.3.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.7.2",
    "tailwindcss": "^2.1.2",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@zerodevx/svelte-toast": "^0.2.2",
    "chart.js": "^3.2.0",
    "concurrently": "^6.0.2",
    "echarts": "^5.1.1",
    "postcss-import": "^14.0.1",
    "sirv-cli": "^1.0.0",
    "svelte-carousel": "^1.0.3",
    "svelte-routing": "^1.6.0"
  }
}
